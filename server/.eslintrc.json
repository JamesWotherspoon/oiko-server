{
  "env": {
    "node": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "google",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "max-len": ["error", { "code": 120 }],
    "new-cap": "off",
    "object-curly-spacing": ["error", "always"],
    "require-jsdoc": "off"
  }
}
